@article{Yan2017_ICS,
abstract = {Electron–photon scattering, or Thomson scattering, is one of the most fundamental mechanisms in electrodynamics, underlying laboratory and astrophysical sources of high-energy X-rays. After a century of studies, it is only recently that sufficiently high electromagnetic field strengths have been available to experimentally study the nonlinear regime of Thomson scattering in the laboratory. Making use of a high-power laser and a laser-driven electron accelerator, we made the first measurements of high-order multiphoton scattering, in which more than 500 near-infrared laser photons were scattered by a single electron into a single X-ray photon. Both the electron motion and the scattered photons were found to depend nonlinearly on field strength. The observed angular distribution of scattered X-rays permits independent measurement of absolute intensity, in situ, during interactions of ultra-intense laser light with free electrons. Furthermore, the experiment's potential to generate attosecond-duration hard X-ray pulses can enable the study of ultrafast nuclear dynamics. T homson scattering (TS) 1 is the elastic scattering of photons by electrons, or the long wavelength limit of Compton scattering. As such, it is a fundamental process in electrodynamics, occurring in many laboratory 2 and astrophysical processes 3 , includ-ing Comptonization of the photon energy spectrum of both the cosmic background radiation and the active galactic nuclei. It can serve as an important diagnostic for measuring high-temperature plasmas in magnetic fusion and high-density plasmas in inertial confinement fusion 4,5 . Moreover, it is also the basis for a class of accelerator-driven high-energy X-ray light sources 6–13},
author = {Yan, Wenchao and Fruhling, Colton and Golovin, Grigory and Haden, Daniel and Luo, Ji and Zhang, Ping and Zhao, Baozhen and Zhang, Jun and Liu, Cheng and Chen, Min and Chen, Shouyuan and Banerjee, Sudeep and Umstadter, Donald},
doi = {10.1038/nphoton.2017.100},
file = {:Users/EG415/Box Sync/Mendeley/papers/Yan et al. - 2017 - High-order multiphoton Thomson scattering - Nature Photonics.pdf:pdf;:Users/EG415/Box Sync/Mendeley/papers//Yan et al. - 2017 - High-order multiphoton Thomson scattering - Nat. Photon.pdf:pdf},
issn = {1749-4885},
journal = {Nature Photonics},
number = {8},
pages = {514--520},
publisher = {Nature Publishing Group},
title = {{High-order multiphoton Thomson scattering}},
url = {http://dx.doi.org/10.1038/nphoton.2017.100 https://www.nature.com/nphoton/journal/vaop/ncurrent/pdf/nphoton.2017.100.pdf http://www.nature.com/doifinder/10.1038/nphoton.2017.100},
volume = {11},
year = {2017}
}

