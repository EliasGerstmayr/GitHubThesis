issn = {0028-0836},
journal = {Nature},
keywords = {before further wave-breaking of,causes an increased,dark-current,energetic bunch,lations behind the first,of,plasma oscil-,short,ters to nature,the plasma is sufficiently,then one can extract,this mono-},
number = {7008},
pages = {538--541},
pmid = {15457252},
title = {{High-quality electron beams from a laser wakefield accelerator using plasma-channel guiding.}},
volume = {431},
year = {2004}
