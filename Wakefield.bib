number = {4},
pages = {043107},
publisher = {AIP Publishing LLC},
title = {{Control of quasi-monoenergetic electron beams from laser-plasma accelerators with adjustable shock density profile}},
url = {http://aip.scitation.org/doi/10.1063/1.5023694 https://doi.org/10.1063/1.5023694{\%}0Ahttp://aip.scitation.org/toc/php/25/4},
volume = {25},
