keywords = {doi:10.1103/PhysRevLett.121.154801 url:https://doi},
title = {{Controlling the Self-Injection Threshold in Laser Wakefield Accelerators}},
url = {https://journals.aps.org/prl/pdf/10.1103/PhysRevLett.121.154801},
volume = {121},
year = {2018}
